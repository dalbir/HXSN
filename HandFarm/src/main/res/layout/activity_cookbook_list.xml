<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/FrameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
           
            android:orientation="vertical" >
   <include layout="@layout/title_back"/>
            <RelativeLayout
                android:id="@+id/discovery_cookbooklist1_RL"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                
                android:background="#FFFFFF" >

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.1dp"
                    android:layout_alignParentTop="true"
                    android:background="#000000" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:layout_toLeftOf="@+id/imageView1"
                    android:src="@drawable/cookbook_list_delete" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:src="@drawable/cooklook_list_search" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView3"
                    android:text="请输入菜名或原料" />

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="14dp"
                    android:src="@drawable/cooklook_list_more" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/discovery_cookbooklist2_RL"
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="#FFFFFF" >

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.1dp"
                    android:layout_alignParentTop="true"
                    android:background="#000000" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/cooklook_food" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView4"
                    android:layout_centerHorizontal="true"
                    android:text="西芹牛柳"
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView4"
                    android:layout_alignLeft="@+id/textView1"
                    android:text="被点12次" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignBottom="@+id/textView1"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="19dp"
                    android:src="@drawable/cookbook_list_hotpepper" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignBottom="@+id/textView3"
                    android:layout_alignLeft="@+id/imageView7"
                    android:src="@drawable/cookbook_list_plus" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/discovery_cookbooklist3_RL"
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:layout_marginTop="0dp"
                android:background="#FFFFFF" >

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.1dp"
                    android:layout_alignParentTop="true"
                    android:background="#000000" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/cooklook_food" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView5"
                    android:layout_centerHorizontal="true"
                    android:text="西芹牛柳" 
                    android:textSize="17dp"/>

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView5"
                    android:layout_alignLeft="@+id/textView4"
                    android:text="被点11次" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignBottom="@+id/textView4"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="17dp"
                    android:src="@drawable/cookbook_list_hotpepper" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignBottom="@+id/textView5"
                    android:layout_alignLeft="@+id/imageView9"
                    android:src="@drawable/cookbook_list_plus" />

            </RelativeLayout>

             <RelativeLayout
                android:id="@+id/discovery_cookbooklist4_RL"
                android:layout_width="match_parent"
                android:layout_height="110dp"
                
                android:background="#FFFFFF" >

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.1dp"
                    android:layout_alignParentTop="true"
                    android:background="#000000" />

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.1dp"
                    android:layout_alignParentBottom="true"
                    android:background="#000000" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:src="@drawable/cooklook_food" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/imageView6"
                    android:layout_centerHorizontal="true"
                    android:text="西芹牛柳" 
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView6"
                    android:layout_alignLeft="@+id/textView6"
                    android:text="被点10次" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignTop="@+id/textView6"
                    android:layout_marginRight="16dp"
                    android:src="@drawable/cookbook_list_hotpepper" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignBottom="@+id/textView7"
                    android:layout_alignLeft="@+id/imageView11"
                    android:src="@drawable/cookbook_list_plus" />

            </RelativeLayout>

            

            
    
    


</LinearLayout>

</FrameLayout>
